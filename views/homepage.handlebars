<div class="container">
    <div class="card">
        {{#each blogs as |blog|}}
        <div class="card-header">
            <h1>{{blog.title}}</h1>
            <p>Posted by: {{user.username}} at {{blog.date}}</p>
            <div class="card-body">
                <p class="text">{{blog.text}}</p>
                <a href="/blogs/{{blog.id}}"></a>
            </div>
        </div>
        {{/each}}
    </div>
</div>

